<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="TextBox.Static.Border" Color="#FFABAdB3"/>
	<SolidColorBrush x:Key="_BlueButton" Color="#FF0073c6"/>
	<SolidColorBrush x:Key="_BlueBright" Color="#FF0094c8"/>
	<SolidColorBrush x:Key="_BlueLuminous" Color="#FF43A4C1"/>
	<SolidColorBrush x:Key="_BlueDark" Color="#FF253858"/>
